<table class="table-center">
  <% locals.items?.forEach(function(item, index) { %>
  <tr>
    <td class="text-left"><%= id %>.<%= index + 1 %>.</td>
    <td><b><%= item.upes_tipas %></b></td>
  </tr>
  <tr>
    <td>Pavadinimas:</td>
    <td class="font-bold"><%= item.upes_pavadinimas %></td>
  </tr>
  <tr>
    <td>Identifikavimo kodas:</td>
    <td class="font-bold"><%= item.upes_kodas %></td>
  </tr>
  <% if (!locals.hideDistanceIfNotExists || !!locals.hideDistanceIfNotExists &&
  !!item.atstumas_nuo_ziociu) { %>
  <tr>
    <td>Atstumas nuo žiočių:</td>
    <td class="font-bold">
      <%= roundNumber(item.atstumas_nuo_ziociu, 1) || '-' %> km
    </td>
  </tr>
  <% }; %> <% }); %>
</table>
